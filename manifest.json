{
   "background": {
      "persistent": false,
      "scripts": [ "scripts/load.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon-16.png",
      "default_title": "CodeFabric Shopify Extensions"
   },
   "description": "Adds functionality and fixes to the Shopify Dashboard (for free!)",
   "homepage_url": "http://codefabric.co.uk",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png"
   },
   "manifest_version": 2,
   "name": "CodeFabric Shopify Extensions",
   "permissions": [ "activeTab" ],
   "short_name": "CodeFabric Shopify",
   "version": "0.0.1"
}
